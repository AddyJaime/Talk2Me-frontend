{
  "expo": {
    "name": "Talk2Me",
    "slug": "Talk2Me",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/Talk2me-logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/images/Talk2me-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "jsEngine": "jsc",
      "newArchEnabled": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "jsEngine": "jsc",
      "newArchEnabled": true
    },
    "web": {},
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": true,
            "bridgeless": true
          },
          "android": {
            "newArchEnabled": true,
            "bridgeless": true
          }
        }
      ]
    ]
  }
}
